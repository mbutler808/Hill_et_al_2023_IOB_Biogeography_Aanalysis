	journal = {Copeia},
	keywords = {Anura ; Art galleries & museums ; Biological taxonomies ; Cloud forests ; Colors ; Deoxyribonucleic acid--DNA ; Forest habitats ; Frogs ; Holotypes ; Iris ; Islands ; Life Sciences & Biomedicine ; Microhylidae ; Mitochondrial DNA ; New species ; Oreophryne ; Paratypes ; Prunus ; s ; Science & Technology ; Young animals ; Zoology},
	language = {eng},
	number = {4},
	pages = {690-697},
	publisher = {The American Society of Ichthyologists and Herpetologists},
	title = {A Remarkable Ontogenetic Change in Color Pattern in a New Species of Oreophryne (Anura: Microhylidae) from Papua New Guinea},
