	journal = {Mol Phylogenet Evol},
	journal-full = {Molecular Phylogenetics and Evolution},
	keywords = {Adaptive radiation; Anura; Asterophryinae; Microhylidae; Systematics},
	mesh = {Animals; Anura; Australia; Bayes Theorem; DNA, Mitochondrial; Evolution, Molecular; Genetic Speciation; Mitochondria; Molecular Typing; New Guinea; Philippines; Phylogeny; Sequence Analysis, DNA},
	month = {07},
	pages = {1-11},
	pmid = {28412536},
	pst = {ppublish},
	title = {Molecular phylogenetics and dating of the problematic New Guinea microhylid frogs (Amphibia: Anura) reveals elevated speciation rates and need for taxonomic reclassification},
	volume = {112},
	year = {2017},
	bdsk-url-1 = {https://doi.org/10.1016/j.ympev.2017.04.008}}

@incollection{Zweifel:1982aa,
	abstract = {The amphibian fauna of New Guinea is comprised entirely of anurans. No caecilian is known closer to New Guinea than the Philippine Is, and the closest salamanders are even more remote ---in southern China. Knowledge of the New Guinean frogs is sufficient to outline their taxonomic and ecological diversity and to support some speculation on patterns of distribution and faunal origins. But detailed analyses of the fauna ---whether from systematic, ecological or biogeographic viewpoints ---are not yet practical. What we present here is intended to give the reader a systematic orientation to the fauna and some appreciation of its ecological and biogeographical complexity.},
