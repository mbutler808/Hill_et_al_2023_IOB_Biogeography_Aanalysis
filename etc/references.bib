	edition = {Received 22 November 2010; revised 26 January 2011; accepted 26 January 2011},
	issn = {0952-8369},
	journal = {Journal of zoology (1987)},
	keywords = {Amphibians ; Analysis ; Animal behavior ; Animal diseases ; Animal populations ; biogeography ; Cytochrome b ; ecological adaptation ; Evolutionary biology ; glacial refugia ; Life Sciences & Biomedicine ; niche separation ; Nonnative species ; phylogeography ; Proteins ; Reptiles & amphibians ; Salamandridae ; Science & Technology ; Zoology},
	language = {eng},
	number = {4},
	pages = {248-256},
	publisher = {Blackwell Publishing Ltd},
	title = {Hybridization of two ancient salamander lineages: molecular evidence for endemic spectacled salamanders on the Apennine peninsula},
	volume = {284},
