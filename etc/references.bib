	keywords = {Amphibians ; Animals ; ecological radiation ; Ecology ; ecomorphology ; Ecosystems ; Environmental aspects ; Environmental Sciences & Ecology ; Evolution ; Evolutionary Biology ; Genetic Speciation ; Genetics & Heredity ; Life Sciences & Biomedicine ; microhabitat ; morphology ; Nuclear radiation ; phylogenetic comparative method ; Phylogeography ; Plethodontidae ; Radiation ; Science & Technology ; Universities and colleges ; Urodela - anatomy & histology ; Urodela - classification ; Urodela - genetics},
	language = {eng},
	number = {4},
