	journal = {Bioinformatics},
	keywords = {Algorithms ; Bayes Theorem ; Biological Evolution ; Computational Biology - methods ; Likelihood Functions ; Models, Biological ; Phylogeny ; Programming Languages},
	language = {eng},
	number = {15},
	pages = {2216-2218},
	title = {geiger v2.0: an expanded suite of methods for fitting macroevolutionary models to phylogenetic trees},
