	journal = {{PROCEEDINGS OF THE NATIONAL ACADEMY OF SCIENCES OF THE UNITED STATES OF AMERICA}},
	journal-iso = {{Proc. Natl. Acad. Sci. U. S. A.}},
	keywords = {{amphibian evolution; macroevolutionary patterns; molecular timetree; paleobiology; phylogenetics}},
	keywords-plus = {{PHYLOGENETIC-RELATIONSHIPS; MASS EXTINCTION; EVOLUTION; TERTIARY; RATES; BIOGEOGRAPHY; GYMNOPHIONA; RADIATION; BOUNDARY; FAMILY}},
	month = {{JAN 16}},
	number = {{3}},
	orcid-numbers = {{Wilkinson, Mark/0000-0002-9459-8976 Roelants, Kim/0000-0001-5367-5958}},
	pages = {{887-892}},
