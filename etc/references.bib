	keywords = {{molecular clock; divergence dating; calibration; biogeography; Bayesian phylogenetics}},
	keywords-plus = {{SPECIES DIVERGENCE TIMES; ESTIMATING EVOLUTIONARY RATES; FOSSIL CALIBRATIONS; BAYESIAN-ESTIMATION; CLIMATE; DIVERSIFICATION; PHYLOGENIES; DISPERSAL; AGE; OLD}},
	month = {{SEP 1}},
	number = {{9}},
	orcid-numbers = {{Ho, Simon YW/0000-0002-0361-2307 Crisp, Michael D/0000-0002-8255-6349 Foster, Charles/0000-0002-6700-3746}},
