	keywords = {Ecological niche modeling; land Change; MaxEnt; Papua New Guinea; plant richness; Western Papua and Papua (Indonesia); SPECIES DISTRIBUTION; NICHE MODELS; DISTRIBUTIONS; BIODIVERSITY; BIOGEOGRAPHY; DEFORESTATION; DEGRADATION; PERFORMANCE; COMPLEXITY; PREDICTION},
