	keywords = {PHYLOGENETIC-RELATIONSHIPS; ANOLIS LIZARDS; CARIBBEAN BIOGEOGRAPHY; BAYESIAN-INFERENCE; BOOTSTRAP MEASURES; IGUANIAN LIZARDS; RAIN-FOREST; EVOLUTION; ECOLOGY; SQUAMATA},
	month = {Jun},
	number = {6},
