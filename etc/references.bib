	keywords = {HUMAN MITOCHONDRIAL-DNA; HISTORICAL BIOGEOGRAPHY; MAXIMUM-LIKELIHOOD; MOLECULAR SYSTEMATICS; DIVERGENCE TIMES; SUBSTITUTION RATES; GEOGRAPHIC RANGE; EVOLUTION; GENUS; ANDES},
	number = {2},
	pages = {257--268},
	publication-type = {J},
