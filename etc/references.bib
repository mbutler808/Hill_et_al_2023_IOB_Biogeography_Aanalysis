	journal = {AMERICAN NATURALIST},
	keywords = {SYMPATRIC STICKLEBACKS GASTEROSTEUS; COOCCURRING DYTISCID BEETLES; DESERT RODENT COMMUNITY; INDIAN ANOLIS LIZARDS; INTERSPECIFIC COMPETITION; FIELD EXPERIMENTS; BRITISH-COLUMBIA; EVOLUTIONARY CONSEQUENCES; MORPHOLOGICAL PATTERNS; PREY COMMUNITIES},
	month = {Oct},
