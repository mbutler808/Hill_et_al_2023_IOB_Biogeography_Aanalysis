	doi = {10.1038/s41598-018-28504-9},
	id = {Noroozi2018},
	isbn = {2045-2322},
	journal = {Scientific Reports},
	number = {1},
	pages = {10345},
	title = {Hotspots within a global biodiversity hotspot - areas of endemism are associated with high mountain ranges},
	url = {https://doi.org/10.1038/s41598-018-28504-9},
	volume = {8},
	year = {2018},
