	issn = {0169-5347},
	journal = {Trends in Ecology \& Evolution},
	number = {12},
	pages = {639 - 644},
	title = {Historical biogeography, ecology and species richness},
	url = {http://www.sciencedirect.com/science/article/B6VJ1-4DFBV49-2/2/344013ce8ab9b0abd26278645afc3aca},
	volume = {19},
