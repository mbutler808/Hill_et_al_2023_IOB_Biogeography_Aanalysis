	iso-source-abbreviation = {Annu. Rev. Ecol. Syst.},
	issn = {0066-4162},
	journal = {ANNUAL REVIEW OF ECOLOGY AND SYSTEMATICS},
	keywords = {AFRICAN FINCH PYRENESTES; MITOCHONDRIAL-DNA SEQUENCES; BILL SIZE POLYMORPHISM; AUSTRALIA WET-TROPICS; RAIN-FOREST REFUGIA; PHYLOGENETIC-RELATIONSHIPS; COMPARATIVE PHYLOGEOGRAPHY; GENE FLOW; GEOGRAPHICAL PATTERNS; ECOLOGICAL GRADIENTS},
	pages = {533--563},
	publication-type = {J},
